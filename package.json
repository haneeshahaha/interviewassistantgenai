{
  "name": "ai-mock-workspace",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "ai-mock",
    "vite-project"
  ],
  "scripts": {
    "dev": "pnpm --filter vite-project run dev",
    "dev:next": "pnpm --filter ai-mock run dev",
    "build": "pnpm --filter vite-project run build",
    "build:next": "pnpm --filter ai-mock run build",
    "lint": "pnpm --filter vite-project run lint",
    "lint:next": "pnpm --filter ai-mock run lint",
    "add:vite": "pnpm --filter vite-project add",
    "add:next": "pnpm --filter ai-mock add",
    "add:dev:vite": "pnpm --filter vite-project add -D",
    "add:dev:next": "pnpm --filter ai-mock add -D"
  },
  "devDependencies": {
    "pnpm": "^8.0.0"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.45.0",
    "react-router-dom": "^7.8.2"
  }
}
